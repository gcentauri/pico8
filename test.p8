pico-8 cartridge // http://www.pico-8.com
version 8
__lua__
   
--utility for loops
--[[ important
use in generic for loops to
iterate over an array(list)]]
function list_iter (t)
 local i, n = 0, #t

 return function ()
  i = i + 1
  if i <= n then 
   return t[i],i,n 
  end
 end
end

--prototype vector object?


--almost polygon?
--[[ 
connect_dots: list [con] [col]
array bool number -> draw

draws a [col]line btw each posn 
in the array. if con is true,
then the last posn is connected
back to the first. --]]

function connect_dots(l,...)
 local con,col = ...
 if (col) color(col)
 for v,i,n in list_iter(l) do
  if (i+1) <= n then
   local v1 = l[i+1]
   line(v.x, v.y, 
        v1.x, v1.y)   
  elseif (con and i==n) then
   local v1 = l[1]
   line(v.x, v.y, 
        v1.x, v1.y)
  else return true end
 end
end

